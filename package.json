		"convex": "^1.27.0",
		"lucide-react": "^0.544.0",
		"next": "15.5.3",
		"next-themes": "^0.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"stream-chat": "^9.19.1",
